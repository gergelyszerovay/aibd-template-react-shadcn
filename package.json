{
  "name": "aibd-template-react-shadcn",
  "version": "0.2.0",
  "description": "A React boilerplate template with Vite, Shadcn and Supabase for AIBD development projects by Gergely Szerovay",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gergelyszerovay/aibd-template-react-shadcn.git"
  },
  "scripts": {
    "dev": "pnpm exec vite --host=127.0.0.1 --port=4300",
    "build": "pnpm exec tsc -b && vite build",
    "preview": "pnpm exec vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "pnpm exec eslint ./src --ext .ts,.tsx",
    "lint:fix": "pnpm exec eslint ./src --ext .ts,.tsx --fix",
    "prettier": "pnpm exec prettier ./src --write",
    "lint-staged": "pnpm exec lint-staged",
    "sheriff-list": "pnpm exec sheriff list",
    "prepare": "pnpm exec husky",
    "madge-image": "pnpm exec madge --ts-config ./tsconfig.app.json --image graph.svg ./src/main.tsx",
    "madge-circular": "pnpm exec madge --ts-config ./tsconfig.app.json --circular ./src/main.tsx",
    "pre-commit-hook": "git hook run pre-commit",
    "typecheck": "pnpm exec tsc -b --noEmit",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "taze-list": "pnpm exec taze major -l",
    "taze-update": "pnpm exec taze major -l -w"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "pnpm exec eslint --quiet --fix",
      "pnpm exec prettier --write"
    ],
    "src/**/*.tsx": [
      "pnpm exec eslint --quiet --fix",
      "pnpm exec prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-aspect-ratio": "1.1.8",
    "@radix-ui/react-avatar": "1.1.11",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-context-menu": "2.2.16",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-hover-card": "1.1.15",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-menubar": "1.1.16",
    "@radix-ui/react-navigation-menu": "1.2.14",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-progress": "1.1.8",
    "@radix-ui/react-radio-group": "1.3.8",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toggle": "1.1.10",
    "@radix-ui/react-toggle-group": "1.1.11",
    "@radix-ui/react-tooltip": "1.2.8",
    "@supabase-cache-helpers/postgrest-react-query": "1.13.7",
    "@supabase/supabase-js": "2.90.1",
    "@tanstack/react-query": "5.90.18",
    "@vitest/browser-playwright": "4.0.17",
    "@xyflow/react": "12.10.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "framer-motion": "12.26.2",
    "input-otp": "1.4.2",
    "jscodeshift": "17.3.0",
    "lucide-react": "0.562.0",
    "next-themes": "0.4.6",
    "react": "19.1.4",
    "react-day-picker": "8.10.1",
    "react-dom": "19.1.4",
    "react-flow": "1.0.3",
    "react-hook-form": "7.71.1",
    "react-resizable-panels": "2.1.9",
    "react-router": "7.12.0",
    "react-router-dom": "7.12.0",
    "recharts": "2.15.3",
    "shadcn": "2.5.0",
    "shadcn-ui": "0.9.5",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "1.0.7",
    "uuid": "13.0.0",
    "vaul": "1.1.2",
    "zod": "4.3.5",
    "zod-validation-error": "5.0.0",
    "zustand": "5.0.10"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "5.0.0",
    "@eslint/js": "9.39.2",
    "@gergelyszerovay/eslint-plugin-path-boundary-imports": "1.0.6",
    "@softarc/eslint-plugin-sheriff": "0.19.6",
    "@softarc/sheriff-core": "0.19.6",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-onboarding": "10.1.11",
    "@storybook/addon-vitest": "10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/browser": "4.0.17",
    "@vitest/coverage-v8": "4.0.17",
    "autoprefixer": "10.4.23",
    "eslint": "9.39.2",
    "eslint-cli": "1.1.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "eslint-plugin-storybook": "10.1.11",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "jsdom": "27.4.0",
    "lint-staged": "16.2.7",
    "madge": "8.0.0",
    "playwright": "1.57.0",
    "postcss": "8.5.6",
    "prettier": "3.8.0",
    "prettier-plugin-organize-imports": "4.3.0",
    "storybook": "10.1.11",
    "supabase": "2.72.7",
    "tailwindcss": "3.4.19",
    "taze": "19.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.0.4",
    "vitest": "4.0.17"
  },
  "packageManager": "pnpm@10.28.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "supabase"
    ]
  }
}
