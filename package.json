{
  "name": "@gergelyszerovay/aibd-template-react",
  "version": "0.1.0",
  "description": "A React boilerplate template with Vite adn Shadcn for AIBD development projects",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "pnpm exec vite --host=127.0.0.1 --port=4300",
    "build": "pnpm exec tsc -b && vite build",
    "preview": "pnpm exec vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "pnpm exec eslint ./src --ext .ts,.tsx",
    "lint:fix": "pnpm exec eslint ./src --ext .ts,.tsx --fix",
    "prettier": "pnpm exec prettier ./src --write",
    "lint-staged": "pnpm exec lint-staged",
    "sheriff-list": "pnpm exec sheriff list",
    "prepare": "pnpm exec husky",
    "madge-image": "pnpm exec madge --ts-config ./tsconfig.app.json --image graph.svg ./src/main.tsx",
    "madge-circular": "pnpm exec madge --ts-config ./tsconfig.app.json --circular ./src/main.tsx"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "pnpm exec eslint --quiet --fix",
      "pnpm exec prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "5.0.1",
    "@radix-ui/react-accordion": "1.2.8",
    "@radix-ui/react-alert-dialog": "1.1.11",
    "@radix-ui/react-aspect-ratio": "1.1.4",
    "@radix-ui/react-avatar": "1.1.7",
    "@radix-ui/react-checkbox": "1.2.3",
    "@radix-ui/react-collapsible": "1.1.8",
    "@radix-ui/react-context-menu": "2.2.12",
    "@radix-ui/react-dialog": "1.1.11",
    "@radix-ui/react-dropdown-menu": "2.1.12",
    "@radix-ui/react-hover-card": "1.1.11",
    "@radix-ui/react-label": "2.1.4",
    "@radix-ui/react-menubar": "1.1.12",
    "@radix-ui/react-navigation-menu": "1.2.10",
    "@radix-ui/react-popover": "1.1.11",
    "@radix-ui/react-progress": "1.1.4",
    "@radix-ui/react-radio-group": "1.3.4",
    "@radix-ui/react-scroll-area": "1.2.6",
    "@radix-ui/react-select": "2.2.2",
    "@radix-ui/react-separator": "1.1.4",
    "@radix-ui/react-slider": "1.3.2",
    "@radix-ui/react-slot": "1.2.0",
    "@radix-ui/react-switch": "1.2.2",
    "@radix-ui/react-tabs": "1.1.9",
    "@radix-ui/react-toggle": "1.1.6",
    "@radix-ui/react-toggle-group": "1.1.7",
    "@radix-ui/react-tooltip": "1.2.4",
    "@supabase/supabase-js": "2.49.4",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "lucide-react": "0.503.0",
    "next-themes": "0.4.6",
    "react": "19.1.0",
    "react-day-picker": "8.10.1",
    "react-dom": "19.1.0",
    "react-hook-form": "7.56.1",
    "react-resizable-panels": "2.1.8",
    "react-router-dom": "7.5.1",
    "recharts": "2.15.3",
    "shadcn": "2.5.0",
    "shadcn-ui": "0.9.5",
    "sonner": "2.0.3",
    "tailwind-merge": "3.2.0",
    "tailwindcss-animate": "1.0.7",
    "vaul": "1.1.2",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@gergelyszerovay/eslint-plugin-path-boundary-imports": "1.0.5",
    "@softarc/eslint-plugin-sheriff": "0.18.0",
    "@softarc/sheriff-core": "0.18.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.1.0",
    "@types/react-dom": "18.3.6",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "3.1.1",
    "autoprefixer": "10.4.21",
    "eslint": "9.25.1",
    "eslint-cli": "1.1.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.5.1",
    "madge": "8.0.0",
    "prettier": "3.5.3",
    "postcss": "8.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "tailwindcss": "3.4.17",
    "taze": "19.0.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "vite": "5.4.17",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  },
  "packageManager": "pnpm@10.9.0"
}
